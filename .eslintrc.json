{
  "extends": "@rocketseat/eslint-config/react",
  "plugins": ["prettier-plugin-tailwindcss", "simple-import-sort"],
  "rules": {
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  }
}